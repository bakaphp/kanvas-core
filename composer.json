{
    "name": "bakaphp/kanvas-core",
    "description": "Kanvas Core Package",
    "type": "library",
    "keywords": [
        "kanvas",
        "mctekk"
    ],
    "homepage": "https://github.com/bakaphp/kanvas-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael White",
            "email": "rwhite@mctekk.com",
            "role": "Backend Developer"
        },
        {
            "name": "Maximo Castro",
            "email": "max@mctekk.com",
            "role": "Backend Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.2",
        "lcobucci/jwt": "^4.1",
        "spatie/data-transfer-object": "^3.7",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.22"
    },
    "autoload": {
        "psr-4": {
            "Kanvas\\": "src",
            "Kanvas\\Database\\": "database"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}