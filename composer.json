{
    "name": "bakaphp/kanvas-core",
    "description": "Kanvas Core Package",
    "keywords": [
        "kanvas",
        "mctekk"
    ],
    "homepage": "https://github.com/bakaphp/kanvas-core",
    "license": "MIT",
    "authors": [{
        "name": "Rafael White",
        "email": "rwhite@mctekk.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.2",
        "spatie/data-transfer-object": "^3.7"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "vimeo/psalm": "^4.22"
    },
    "autoload": {
        "psr-4": {
            "Kanvas\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}